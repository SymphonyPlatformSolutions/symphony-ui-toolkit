{
  "name": "@symphony-ui/uitoolkit",
  "private": true,
  "version": "4.0.6",
  "description": "React components for the Symphony UI design system",
  "workspaces": [
    "packages/*"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "author": "Symphony",
  "license": "ISC",
  "scripts": {
    "dedupe": "npx yarn-deduplicate yarn.lock",
