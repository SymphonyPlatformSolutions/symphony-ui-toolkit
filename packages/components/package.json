{
  "name": "@symphony-ui/uitoolkit-components",
  "version": "4.8.1",
  "description": "React components for the Symphony UI design system",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "license": "ISC",
  "sideEffects": false,
  "main": "index.js",
  "scripts": {
    "prepare": "cd ../.. && husky install",
    "clean": "rm -rf dist",
    "dedupe": "yarn-deduplicate",
    "init": "mkdir dist",
    "prebuild": "yarn run clean && yarn run init",
    "build": "yarn svg-icons-to-react-components && tsc && yarn build-esm",
    "build-esm": "tsc --project ./tsconfig.esm.json",
    "svg-icons-to-react-components": "svgr --icon --out-dir src/components/icons -- ../styles/src/icons/svg",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx ./",
    "lint-fix": "eslint --ext .ts,.tsx,.js,.jsx ./ --fix",
    "start": "storybook dev -p 6006",
    "storybook-docs": "storybook dev --docs",
    "build-storybook": "storybook build -c .storybook -o dist/storybook",
    "test": "jest --watchAll",
    "test-once": "jest -w 1",
    "cover": "jest -w 1 --coverage",
    "check-circular": "madge --circular --extensions ts,tsx ./",
    "linkit": "yarn build && cp package.json ./dist && cd ./dist && yarn link && cd .."
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "engines": {
    "node": ">=14.21.2"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.4",
    "@floating-ui/react": "^0.26.20",
    "@popperjs/core": "^2.4.4",
    "@symphony-ui/uitoolkit-styles": "4.8.1",
    "clsx": "^2.1.1",
    "date-fns": "^2.16.1",
    "emoji-js": "^3.5.0",
    "github-markdown-css": "^4.0.0",
    "prop-types": "^15.8.1",
    "react-resize-detector": "^5.0.3",
    "react-select": "^4.3.1",
    "react-virtualized": "^9.22.2",
    "shortid": "^2.2.16"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@storybook/addon-a11y": "^7.5.3",
    "@storybook/addon-actions": "^7.5.3",
    "@storybook/addon-controls": "^7.5.3",
    "@storybook/addon-docs": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/addon-styling-webpack": "^0.0.5",
    "@storybook/icons": "^1.2.9",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/preview-api": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-webpack5": "^7.5.3",
    "@storybook/theming": "^7.5.3",
    "@svgr/cli": "^6.5.1",
    "@testing-library/dom": "^7.29.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0",
    "@types/enzyme": "^3.10.5",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-select": "^4.0.17",
    "@types/react-virtualized": "^9.21.10",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "css-loader": "^4.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^8.51.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "html-react-parser": "^0.14.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-extended": "^0.11.5",
    "lint-staged": "^12.1.2",
    "madge": "^6.1.0",
    "markdown-loader": "^6.0.0",
    "mdx-embed": "^1.1.2",
    "prettier": "^2.2.1",
    "react": "^16.13.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-docgen-typescript-plugin": "^1.0.5",
    "react-dom": "^16.13.1",
    "react-hooks-testing-library": "^0.6.0",
    "sass": "^1.26.11",
    "sass-loader": "^10.0.2",
    "storybook": "^7.5.3",
    "storybook-addon-mdx-embed": "^1.1.1",
    "storybook-addon-pseudo-states": "^3.0.1",
    "storybook-addon-themes": "^6.1.0",
    "storybook-dark-mode": "^3.0.1",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.5",
    "typescript": "^5.2.2",
    "yarn-deduplicate": "^3.1.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "eslint --cache --fix",
    "*.{css,md}": "prettier --write"
  }
}
