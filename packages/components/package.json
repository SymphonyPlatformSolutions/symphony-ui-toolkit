{
  "name": "@symphony-ui/uitoolkit-components",
  "version": "3.0.4",
  "description": "React components for the Symphony UI design system",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "license": "ISC",
  "sideEffects": false,
  "main": "index.js",
  "scripts": {
    "prepare": "cd ../.. && husky install",
    "clean": "rm -rf dist",
    "dedupe": "yarn-deduplicate",
    "init": "mkdir dist",
    "prebuild": "yarn run clean && yarn run init",
    "build": "tsc && yarn build-esm",
    "build-esm": "tsc --project ./tsconfig.esm.json",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx ./",
    "lint-fix": "eslint --ext .ts,.tsx,.js,.jsx ./ --fix",
    "start": "start-storybook -p 6006",
    "storybook-docs": "start-storybook --docs",
    "build-storybook": "build-storybook -c .storybook -o dist/storybook",
    "test": "jest --watchAll",
    "test-once": "jest -w 1",
    "cover": "jest -w 1 --coverage",
    "check-circular": "madge --circular --extensions ts,tsx ./",
    "linkit": "yarn build && cp package.json ./dist && cd ./dist && yarn link && cd .."
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.4.4",
    "@seznam/compose-react-refs": "^1.0.6",
    "@symphony-ui/uitoolkit-styles": "3.0.4",
    "@types/classnames": "^2.2.10",
    "classnames": "^2.2.6",
    "date-fns": "^2.16.1",
    "emoji-js": "^3.5.0",
    "github-markdown-css": "^4.0.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "react-cool-onclickoutside": "^1.5.8",
    "react-popper": "^2.2.3",
    "react-resize-detector": "^5.0.3",
    "react-select": "^4.3.1",
    "react-transition-group": "^4.4.1",
    "react-virtualized": "^9.22.2",
    "shortid": "^2.2.16",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@storybook/addon-a11y": "^6.0.26",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-controls": "^6.0.21",
    "@storybook/addon-docs": "^6.0.27",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/addons": "^6.0.21",
    "@storybook/preset-scss": "^1.0.2",
    "@storybook/react": "^6.0.21",
    "@testing-library/dom": "^7.29.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.154",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-select": "^4.0.17",
    "@types/react-virtualized": "^9.21.10",
    "@types/storybook__react": "^5.2.1",
    "@types/styled-components": "5.1.21",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "css-loader": "^4.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.1.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "html-react-parser": "^0.14.0",
    "husky": "^7.0.4",
    "jest": "^26.0.1",
    "jest-extended": "^0.11.5",
    "lint-staged": "^12.1.2",
    "madge": "^4.0.0",
    "markdown-loader": "^6.0.0",
    "mdx-embed": "^0.0.22",
    "prettier": "^2.2.1",
    "react": "^16.13.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.13.1",
    "react-hooks-testing-library": "^0.6.0",
    "sass": "^1.26.11",
    "sass-loader": "^10.0.2",
    "storybook-addon-mdx-embed": "^0.0.22",
    "storybook-addon-themes": "^6.1.0",
    "storybook-dark-mode": "^1.0.3",
    "style-loader": "^1.2.1",
    "ts-jest": "^26.0.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.3",
    "yarn-deduplicate": "^3.1.0"
  },
  "resolutions": {
    "**/lodash": "^4.17.21"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "eslint --cache --fix",
    "*.{css,md}": "prettier --write"
  }
}